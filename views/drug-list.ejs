<%- include("partials/header") -%>
<form action="/" method="post">
  <button type="submit" name="name" value="homePage" class="top-left fas fa-<%= animal %> fa-3x"></button>
</form>
<p class="weightText">
  <%= weight %> kgs
  </p>
<h1><%= category %> (Dosages in mg/kg)</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Drug</th>
      <th scope="col">Method</th>
      <th scope="col">Frequency</th>
      <th scope="col">Min</th>
      <th scope="col">Max</th>
    </tr>
  </thead>
  <tbody>
    <% drugs.forEach(function(drug){ %>
    <tr>
      <th scope="row"><%= drug.drugName %></th>
      <td><%= drug.drugDetails.Method %></td>
      <td><%= drug.drugDetails.Frequency %></td>
      <td><%= drug.drugDetails.maxAmount %></td>
      <td><%= drug.drugDetails.maxAmount %></td>
    </tr>
    <% }); %>

  </tbody>
</table>



<%- include("partials/footer") -%>
